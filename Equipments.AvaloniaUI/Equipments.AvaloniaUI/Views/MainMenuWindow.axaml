<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:rxui="http://reactiveui.net"
		xmlns:dialogHost="clr-namespace:DialogHostAvalonia;assembly=DialogHost.Avalonia"
		xmlns:vm="clr-namespace:Equipments.AvaloniaUI.ViewModels"
		xmlns:views="clr-namespace:Equipments.AvaloniaUI.Views"
		mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="Equipments.AvaloniaUI.Views.MainMenuWindow"
		Icon="/Assets/Icon.ico"
        Title="Система управления оборудованием"
		Padding="{Binding $self.OffScreenMargin}"
		x:DataType="vm:MainMenuWindowViewModel"
		WindowStartupLocation="CenterScreen"
		Background="{DynamicResource BaseTheme}">
	<views:MainMenuView/>
</Window>
